{
	"name": "neura",
	"version": "1.0.0",
	"description": "neura",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"start": "ts-node src/Start.ts",
		"stop": "ts-node src/Stop.ts",
		"echo": "ts-node text-to-speech.ts",
		"watch": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
		"docker-build": "cd coqui-tts-cpu && docker build -t coqui .",
		"docker-run": "docker run -d --name coqui -p 7860:7860 coqui",
		"coqui": "ts-node coqui-tts.ts",
		 "docker-push": "npm run docker-build && docker tag coqui scaineio/coqui:latest && docker push scaineio/coqui:latest"
	},
	"dependencies": {
		"axios": "^1.12.2",
		"dotenv": "^17.2.2"
	},
	"devDependencies": {
		"@gradio/client": "^1.19.0",
		"nodemon": "^2.0.0",
		"ts-node": "^10.0.0",
		"typescript": "^4.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/scaine-io/neura.git"
	},
	"keywords": [],
	"author": "Scaine",
	"license": "MIT"
}
